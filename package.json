{
  "name": "react-custom-hooks",
  "displayName": "React Custom Hooks",
  "description": "Insert custom react hooks to your project with autocomplete support",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.92.0"
  },
  "publisher": "zeesec",
  "license": "MIT",
  "author": {
    "email": "iamzeeshan1233@gmail.com",
    "name": "Muhammad Zeeshan",
    "url": "https://linkedin.com/in/zeesec"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#e3f4ff",
    "theme": "light"
  },
  "keywords": [
    "reactcustomhooks",
    "rch",
    "react",
    "customhooks",
    "hooks",
    "vscodereact",
    "vscodereacthooks",
    "reacthooks",
    "vscodehooks"
  ],
  "maintainers": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/itszeeshan/react-custom-hooks"
  },
  "bugs": {
    "url": "https://github.com/itszeeshan/react-custom-hooks/issues",
    "email": "iamzeeshan1233@gmail.com"
  },
  "categories": [
    "Programming Languages",
    "Language Packs",
    "Snippets",
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-custom-hooks.helloWorld",
        "title": "Hello World"
      }
    ],
    "languages": [
      {
        "id": "javascript",
        "aliases": [
          "JavaScript",
          "javascript"
        ],
        "extensions": [
          ".js",
          ".jsx"
        ],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "typescript",
        "aliases": [
          "TypeScript",
          "typescript"
        ],
        "extensions": [
          ".ts",
          ".tsx"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "build": "vsce package",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "postformat": "npm run lint -- --fix",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.1.7",
    "@semantic-release/npm": "^12.0.1",
    "@types/node": "20.x",
    "@types/vscode": "^1.92.0",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.1",
    "esbuild": "^0.21.5",
    "eslint": "^8.57.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "semantic-release": "^24.1.0",
    "typescript": "^5.4.5",
    "vsce": "^2.15.0"
  }
}
